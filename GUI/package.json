    "vue": "^2.6.14",
    "vue-draggable-resizable": "^2.3.0",
    "vue-router": "^3.5.3",
    "vue-slider-component": "^3.2.15",
    "vue-vanilla-datetime-picker": "^1.2.0",
    "vuetify": "^2.6.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.8",
