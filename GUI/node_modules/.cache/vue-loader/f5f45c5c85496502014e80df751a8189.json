d by applicable law or agreed to in writing, software\n  - distributed under the License is distributed on an \"AS IS\" BASIS,\n  - WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.\n  - See the License for the specific language governing permissions and\n  - limitations under the License.\n  -\n  -->\n\n<template>\n    <div class=\"form-wrapper\">\n        <div class=\"slider-wrapper secondary-heading\">\n            <label class=\"\">{{settings.title}}</label>\n            <ToggleSwitch :id=\"settings.id\" v-model=\"switchIsOn\" :change=\"updateSliderFilters()\"></ToggleSwitch>\n        </div>\n        <div v-show=\"switchIsOn\" class=\"row-wrapper\">\n            <Slider :id=\"settings.id\" :config=\"settings.config\" @sliderChange=\"updateSliderValue($event)\"></Slider>\n        </div>\n    </div>\n</template>\n\n<script>\n    import ToggleSwitch from '@/components/ToggleSwitch';\n    import Slider from '@/components/Slider';\n    import {mapActions} from 'vuex';\n\n    export default {\n        name: 'SliderFilter',\n        props: {\n            settings: Object\n        },\n        data: () => ({\n            switchIsOn: false\n        }),\n        components: {ToggleSwitch, Slider},\n        methods: {\n            ...mapActions(['removeFilter', 'updateFilters']),\n            updateSliderValue(value) {\n                if(value) {\n                    let sliderObject = {value: value};\n                    this.updateFilters({type: this.settings.id, filter: sliderObject});\n                }\n            },\n            updateSliderFilters() {\n                if(!this.switchIsOn) {\n                    //If a slider filter exists of this type remove the filter\n                    if(this.$store.getters.getFilterByType(this.settings.id)) {\n                        this.removeFilter({type: this.settings.id});\n                    }\n                }\n                else {\n                    this.updateSliderValue(this.settings.config.value);\n                }\n            }\n        }\n    }\n</script>\n\n<style  lang=\"scss\" scoped>\n    .slider-wrapper {\n        @include flex-row-nowrap;\n        align-items: center;\n        justify-content: space-between;\n    }\n</style>\n\n"]}]}