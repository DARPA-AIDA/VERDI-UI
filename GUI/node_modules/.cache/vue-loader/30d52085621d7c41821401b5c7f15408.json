er\">\n        <span v-if=\"condition\" :class=\"conditionClass\"> {{condition}} </span>\n        <div class=\"autocomplete-input\">\n            <AutoComplete class=\"filter-input\"\n                          :items=\"categoryValues\"\n                          :required=\"false\"\n                          :placeholder=\"'Any ' + categoryDetails.label\"\n                          :message=\"'No ' + categoryDetails.message + ' available'\"\n                          @autoCompleteSubmission=\"$emit('categoryValueChange', $event)\"\n                          @autoCompleteClear=\"$emit('categoryValueChange', $event)\"\n            ></AutoComplete>\n        </div>\n        <span class=\"icon-text\"> {{categoryDetails.preposition}} </span>\n        <div class=\"autocomplete-input\">\n            <AutoComplete class=\"filter-input\"\n                          :items=\"roleValues\"\n                          :required=\"false\"\n                          :placeholder=\"'Any Role'\"\n                          :message=\"'No roles available'\"\n                          @autoCompleteSubmission=\"$emit('roleValueChange', $event)\"\n                          @autoCompleteClear=\"$emit('roleValueChange', $event)\"\n            ></AutoComplete>\n        </div>\n    </div>\n</template>\n\n<script>\n    import AutoComplete from '@/components/AutoComplete';\n\n    export default {\n        name: 'EntityRoleFilter',\n        components: {AutoComplete},\n        props: {\n            categoryDetails: {\n                type: Object,\n                required: false,\n                default() {\n                    return {\n                        preposition: 'AS',\n                        label: 'Category',\n                        message: 'category'\n                    }\n                }\n            },\n            categoryValues: {\n                type: Array,\n                required: true\n            },\n            roleValues: {\n                type: Array,\n       