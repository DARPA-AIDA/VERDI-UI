{"remainingRequest":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/womitowoju/workspace/aida/VERDI-UI/GUI/src/App.vue?vue&type=style&index=0&lang=scss&","dependencies":[{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/src/App.vue","mtime":1640207172502},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/css-loader/dist/cjs.js","mtime":1641411773021},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":1641411774047},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/postcss-loader/src/index.js","mtime":1641411773350},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/sass-loader/dist/cjs.js","mtime":1641411772200},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/cache-loader/dist/cjs.js","mtime":1641411772188},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/vue-loader/lib/index.js","mtime":1641411773606}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["App.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+SA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA","file":"App.vue","sourceRoot":"src","sourcesContent":["<!--\n  - Copyright 2019 Next Century Corporation/CACI\n  -\n  - Licensed under the Apache License, Version 2.0 (the \"License\");\n  - you may not use this file except in compliance with the License.\n  - You may obtain a copy of the License at\n  -\n  -       http://www.apache.org/licenses/LICENSE-2.0\n  -\n  - Unless required by applicable law or agreed to in writing, software\n  - distributed under the License is distributed on an \"AS IS\" BASIS,\n  - WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.\n  - See the License for the specific language governing permissions and\n  - limitations under the License.\n  -\n  -->\n\n<template>\n    <div id=\"app\" :class=\"fullPageWidthFilter ? 'app-container' : 'app-container menu-width-filter'\" data-app>\n        <div id=\"app_layout\" class=\"v-application\">\n            <v-app-bar app class=\"app-header\" id=\"app_header\">\n                <router-link class=\"primary-nav-links\" to=\"/\">\n                    <div class=\"program-title\">\n                        <img alt=\"AIDA Logo\" class=\"program-icon\" :src=\"showProgramTitle ? titleImage: iconImage\">\n                    </div>\n                </router-link>\n                <div class=\"primary-nav-routes\">\n                    <router-link class=\"primary-nav-links\" to=\"/discover\">Discover</router-link>\n                    <router-link class=\"primary-nav-links\" to=\"/explore\">Explore</router-link>\n                    <router-link class=\"primary-nav-links\" to=\"/analyze\">Analyze</router-link>\n                    <router-link class=\"primary-nav-links\" to=\"/claims\">Claims</router-link>\n                    <router-link class=\"primary-nav-links\" to=\"/eval\">Evaluation</router-link>\n                </div>\n                <div v-if=\"!hideNavBarInputs\" class=\"app-search-menu\">\n                    <div class=\"data-source-dropdown\">\n                        <SelectDropdown class=\"dropdown-selector\"\n                            :options=\"dataSources\"\n                            :selectedOption=\"currentSource\"\n                            :selectDisplay=\"selectDisplay\"\n                            @selectDropdownChange=\"updateDataSource($event.target.value)\"\n                        >\n                        </SelectDropdown>\n                    </div>\n                    <SearchBar v-if=\"!hideSearchBar\" :showAutoComplete=\"ta2Source\"></SearchBar>\n                </div>\n                <div class=\"primary-nav-icons\">\n                    <button class=\"light-icon-button\" @click.stop=\"toggleProfileMenu()\">\n                        <v-icon id=\"profile_menu_icon\">mdi-dots-vertical</v-icon>\n                    </button>\n                </div>\n                <TransitionMenu ref=\"profile_dropdown_menu\"  v-show=\"showProfileOptions\" :menuData=\"profileMenuProps\" @transitionMenuOptionSelected=\"onProfileMenuSelection\"></TransitionMenu>\n            </v-app-bar>\n\n            <div id=\"app_content\" ref=\"app_content\" class=\"app-content\">\n                <!-- Only display search bar on views other than the home page-->\n                <SecondaryNavigation v-if=\"routeChange(queryPageActive)\" :showFilter=showFilter @updateShowFilter=\"showFilter = $event\"></SecondaryNavigation>\n                <div class=\"app-content-display\"  :class=\"queryPageActive ? 'below-secondary' : 'below-primary'\">\n                    <!-- Using vue-router to route between views-->\n                    <div v-if=\"queryPageActive\">\n                        <transition name=\"slide-in\">\n                            <FilterMenu ref=\"filter_menu\" v-show=\"showFilter\" @updateShowFilter=\"showFilter = $event\"></FilterMenu>\n                        </transition>\n                    </div>\n                    <div v-if=\"currentDataSource || this.$router.currentRoute.path === '/settings/graphs'\" class=\"full-width\">\n                        <router-view />\n                    </div>\n                </div>\n            </div>\n\n            <div ref=\"app_footer\" class=\"footer-container\" data-booted=\"true\">\n                <div class=\"footer-width-limit\">\n                    <div ref=\"footer_nav\" class=\"footer\">\n                        <div class=\"footer-text-wrapper\">\n                            <div class=\"footer-sponsors\">\n                                <div  class=\"sponsor-wrapper\">\n                                    <div class=\"footer-icon-wrapper\">\n                                        <router-link class=\"primary-nav-links\" to=\"/\">\n                                            <div class=\"program-title\">\n                                                <img alt=\"AIDA Logo\" class=\"program-icon\" src=\"@/assets/images/verdi-nocircle.svg\">\n                                            </div>\n                                        </router-link>\n                                    </div>\n                                    <div class=\"footer-content sponsor-content\">\n                                        <p class=\"footer-header\"> {{entries.sponsor.header}}:</p>\n                                        <div v-for=\"(sponsor_content, sponsor_content_key) in entries.sponsor.contents\" :key=\"entries.sponsor.header + '-' + sponsor_content_key\">\n                                            <span :class=\"sponsor_content.type==='title' ? 'sponsor-title' : ''\">{{sponsor_content.value}}</span>\n                                        </div>\n                                    </div>\n                                    <div class=\"footer-content\">\n                                        <div v-for=\"(links_content, links_content_key) in entries.sponsor_links.contents\" :key=\"entries.sponsor_links.header + '-' + links_content_key\">\n                                            <a  class=\"sponsor-link\" :href=\"links_content.link\" target=\"-blank\">{{links_content.value}}</a>\n                                        </div>\n                                    </div>\n                                    <div class=\"footer-content\">\n                                        <div v-for=\"(site_content, site_content_key) in entries.site_info.contents\" :key=\"site_content_key\">\n                                            <span>{{site_content.value}}</span>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"footer-performers\">\n                                <div class=\"performer-wrapper\">\n                                    <div class=\"footer-content performer-content\" v-for=\"(entry, key) in entries.performers\" :key=\"key\">\n                                        <p v-if=\"entry.header\" class=\"footer-header\">{{entry.header}}:</p>\n                                        <p v-else></p>\n                                        <div v-for=\"(content, contentKey) in entry.contents\" :key=\"entry.header + '-' + contentKey\">\n                                            <span v-if=\"content.type === 'text'\">{{content.value}}</span>\n                                            <a v-if=\"content.type === 'link'\" :href=\"content.link\" target=\"-blank\">{{content.value}}</a>\n                                            <a v-if=\"content.type === 'email'\" :href=\"'mailto:' + content.value\">{{content.value}}</a>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import acknowledgements from '@/assets/data/acknowledgements.json';\n    import SearchBar from '@/components/SearchBar';\n    import FilterMenu from '@/components/FilterMenu';\n    import API from './api';\n    import TransitionMenu from '@/components/TransitionMenu';\n    import SecondaryNavigation from '@/components/SecondaryNavigation';\n    import SelectDropdown from '@/components/SelectDropdown';\n    import {SELECTDISPLAY, PARAMETERNAMES, VUEROUTES, DATASOURCEURI, ta2DataSource, ta3DataSource} from './utils';\n    import titleImage from '@/assets/images/verdi-acronym.svg';\n    import iconImage from '@/assets/images/verdi-lion.png';\n    import QuerySubmissionMixin from '@/mixins/QuerySubmissionMixin';\n    import RouteParametersMixin from '@/mixins/RouteParametersMixin';\n    import {mapState, mapMutations, mapActions, mapGetters} from 'vuex';\n\n    let requiredHomePageHeight = 920;\n    let requiredWindowHeight = 1380;\n    let requiredWindowWidth = 681;\n    let profileElement = null;\n    let footerElement = null;\n\n    export default {\n        name: 'App',\n        mixins: [QuerySubmissionMixin, RouteParametersMixin],\n        components: {SearchBar, FilterMenu, TransitionMenu, SecondaryNavigation, SelectDropdown},\n        data: () => ({\n            showProfileOptions: false,\n            showFilter: true,\n            showProgramTitle: true,\n            titleImage,\n            iconImage,\n            profileMenuProps: {\n                transitionType : 'slide-fade',\n                menuClasses : 'vertical-dots-menu profile-dropdown-menu',\n                menuOptions : [\n                    {path:'savedFilters', icon: 'mdi-tune', value: 'Saved Filters', active: false},\n                    {path:'savedBookmarks', icon: 'mdi-bookmark-outline', value: 'Saved Bookmarks', active: false},\n                    {path:'accountProfile', icon: 'mdi-account-outline', value: 'Account Profile', active: false},\n                    {path:'settings/graphs', icon: 'mdi-graphql', value: 'Graph Management', active: true},\n                    {path:'settings', icon: 'mdi-settings-outline', value: 'Settings', active: false}\n                ]\n            },\n            selectDisplay: ''\n        }),\n        computed: {\n            ...mapState(['currentDataSource', 'dataSourceList']),\n            ...mapGetters(['getStoreByRoute']),\n            entries() {\n                return acknowledgements;\n            },\n            homePageInactive() {\n                return this.$route.name !== 'home' && this.$route.name !== null;\n            },\n            hideNavBarInputs() {\n                return this.$route.name === 'eval' || this.$route.name === 'incidentDetail' || this.$route.name === 'claims';\n            },\n            hideSearchBar() {\n                return this.$route.name !== 'explore';\n            },\n            queryPageActive() {\n                //TODO: Analyze filters are not implemented so TA3 route has been removed. TA3 will be added back in once filters are working.\n                return this.$route.name === VUEROUTES.ta2 || this.$route.name === VUEROUTES.claims; //|| this.$route.name === VUEROUTES.ta3;\n            },\n            fullPageWidthFilter() {\n                return this.$route.name === 'eval';\n            },\n            dataSources() {\n                return this.dataSourceList.map(this.truncateSource);\n            },\n            currentSource() {\n                return this.currentDataSource && this.truncateSource(this.currentDataSource);\n            },\n            ta2Source() {\n                return ta2DataSource(this.currentDataSource);\n            }\n        },\n        created() {\n            this.selectDisplay = SELECTDISPLAY.text;\n            API.setInitialData();\n            //added for checking window resize\n            window.addEventListener('resize', this.adjustContentOnChange);\n            this.VUEROUTES = VUEROUTES;\n        },\n        mounted() {\n            profileElement = this.$refs.profile_dropdown_menu;\n            footerElement = this.$refs.app_footer;\n            this.adjustContentOnChange();\n        },\n        destroyed() {\n            window.removeEventListener('resize', this.adjustContentOnChange);\n        },\n        methods: {\n            ...mapMutations(['removeAllFilters', 'updateFilterReset']),\n            ...mapActions(['updateSearchTerm', 'updateCurrentDataSource']),\n            //check to see if v-content has a height greater than 589 then add fixed footer class\n            adjustContentOnChange() {\n                if(footerElement) {\n                    if(((!this.homePageInactive && window.innerHeight >= requiredHomePageHeight) ||\n                        (this.homePageInactive && window.innerHeight >= requiredWindowHeight)) &&\n                        !footerElement.classList.contains('v-footer--fixed'))\n                    {\n                        footerElement.classList.add('v-footer--fixed');\n                    }\n                    else if (((!this.homePageInactive && window.innerHeight < requiredHomePageHeight) ||\n                        (this.homePageInactive && window.innerHeight < requiredWindowHeight)) &&\n                        footerElement.classList.contains('v-footer--fixed'))\n                    {\n                        footerElement.classList.remove('v-footer--fixed');\n                    }\n                }\n\n                this.showProgramTitle = window.innerWidth >= requiredWindowWidth;\n            },\n            routeChange(route) {\n                this.adjustContentOnChange();\n                const routeByPath = this.pathRouteType();\n                const storeByRoute = this.getStoreByRoute(routeByPath);\n                if(this.$route.query.hasOwnProperty(PARAMETERNAMES.datasoure)) {\n                    if(this.currentDataSource !== this.$route.query[PARAMETERNAMES.datasoure]) {\n                        this.updateCurrentDataSource(this.$route.query[PARAMETERNAMES.datasoure]);\n                    }\n                }\n                else if(this.dataSourceRouteType() !== routeByPath) {\n                    this.updateCurrentDataSource(storeByRoute);\n                    this.setQueryType(this.getDefaultTypeIdByRoute());\n                }\n\n                return route;\n            },\n            //closes the profile menu when the user clicks anywhere on the page other than the menu button\n            closeProfileMenu() {\n                if(profileElement && this.showProfileOptions) {\n                    this.showProfileOptions = false;\n                    document.removeEventListener('click', this.closeProfileMenu);\n                }\n            },\n            pathRouteType() {\n                return this.$route.path.includes(VUEROUTES.ta3) ? VUEROUTES.ta3 : VUEROUTES.ta2;\n            },\n            onProfileMenuSelection() {\n                this.closeProfileMenu();\n            },\n            toggleProfileMenu() {\n                if(!this.showProfileOptions) {\n                    document.addEventListener('click', this.closeProfileMenu);\n                    this.showProfileOptions = true;\n                }\n                else{\n                    this.closeProfileMenu();\n                }\n            },\n            updateDataSource(source) {\n                //When the data source is updated, the page will reroute to its respective view.\n                //The query params and results will be reset.\n                let sourcePath = DATASOURCEURI + source;\n                let routeType = VUEROUTES.ta2;\n                this.updateCurrentDataSource(sourcePath);\n                this.updateSearchTerm({});\n                this.updateRouteParams(PARAMETERNAMES.term);\n\n                if(ta2DataSource(source)) {\n                    API.setCategoryData(sourcePath);\n                }\n                else if(ta3DataSource(source)) {\n                    routeType = VUEROUTES.ta3;\n                }\n\n                this.removeAllFilters();\n                this.updateFilterReset(true);\n                this.updateRouteParams(PARAMETERNAMES.filter);\n                this.updateRouteParams(PARAMETERNAMES.datasoure, sourcePath, routeType);\n                this.resetAllData();\n            },\n            truncateSource(source) {\n                return source.replace(DATASOURCEURI, '');\n            }\n        }\n    };\n\n</script>\n<style lang=\"scss\">\n\n    .v-application{\n        font-family: Arial, Helvetica, sans-serif !important;\n        @include flex-column-nowrap;\n    }\n\n    .app-container {\n        -webkit-font-smoothing: antialiased;\n        -moz-osx-font-smoothing: grayscale;\n        color: $theme-primary-color !important;\n        font-size: 14px;\n        line-height: 1.5;\n    }\n\n    .menu-width-filter > .v-autocomplete__content.v-menu__content{\n        min-width: unset !important;\n        display: inline-flex;\n        background-color: white;\n        left: 6px !important;\n    }\n\n    .app-header{\n        background-color: $theme-teal !important;\n        height: unset !important;\n    }\n\n    .app-search-menu{\n        @include flex-row-nowrap;\n        flex: 1 1 auto;\n        align-items: center;\n    }\n\n    .v-toolbar__content{\n        height: unset !important;\n        overflow-x: auto;\n        min-height: 95px;\n    }\n\n    .app-content {\n        /*padding added to account for the primary title bar*/\n        padding:$show-below-header 0 0 !important;\n    }\n\n    .app-content-display{\n        min-height: $full-page-min-height;\n        display: flex;\n        flex: 0 0;\n    }\n\n    .below-primary {\n        /*padding added to account for the primary nav bar */\n        padding-top: $content-below-searchbar;\n    }\n\n    .below-secondary {\n        /*padding added to account for the secondary nav bar*/\n        padding-top: $content-below-filter;\n    }\n\n    .program-title {\n        @include flex-row-nowrap;\n        align-items: center;\n    }\n\n    .program-icon {\n        height: 68px;\n    }\n\n    .primary-nav-routes {\n        @include flex-row-wrap;\n        justify-content: space-evenly;\n        align-items: center;\n        flex: 0 1 380px;\n        margin-left: 6px;\n\n        .router-link-active {\n            font-weight: bold;\n            color: $theme-gold !important\n        }\n    }\n\n    .primary-nav-links {\n        @extend .page-link;\n        @include no-text-decoration;\n        color:$theme-dark-light-color;\n        margin-right: 4px;\n        max-width: 192px;\n    }\n\n    .primary-nav-icons{\n        margin-left: auto;\n    }\n\n    .primary-nav-icons.v-icon{\n        color: $theme-dark-light-color !important;\n    }\n\n    .data-source-dropdown {\n        background-color: $theme-dark-light-color;\n        z-index: 3;\n        border-radius: 5px;\n        margin: 5px 10px;\n        height: 36px;\n    }\n\n    .dropdown-selector {\n        max-width: 465px;\n    }\n\n    .profile-dropdown-menu {\n        top: 60px;\n        z-index:5;\n    }\n\n    .footer-container {\n        left: 0;\n        right: 0;\n        bottom: 0;\n        align-items: center;\n        display: flex;\n        flex: 0 1 auto !important;\n        flex-wrap: wrap;\n        transition-duration: 0.2s;\n        transition-property: background-color, left, right;\n        transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);\n        color: $theme-dark-medium-color;\n        justify-content: center;\n        overflow-y: auto;\n        max-height: 450px;\n        background: url(assets/images/footer.png) left no-repeat;\n        background-color: $theme-dark-offset;\n        background-position: 0 100%;\n        background-size: 100%;\n    }\n\n    .footer-width-limit {\n        overflow-x: auto;\n        width: 100%;\n        display: flex;\n        justify-content: center;\n    }\n\n    .footer {\n        font-size: 13px;\n        @include flex-row-nowrap;\n        justify-content: space-evenly;\n        width: 84%;\n        padding: 20px 0;\n        min-width: 410px;\n    }\n\n    .footer-content {\n        margin: 6px 6px;\n    }\n\n    .footer-header {\n        margin-bottom: 4px !important;\n        font-weight: bold;\n    }\n\n    .footer-icon-wrapper{\n        display: flex;\n        margin: 14px 0;\n    }\n\n    .footer-text-wrapper{\n        @include flex-row-nowrap;\n        width: 100%;\n    }\n\n    .footer-sponsors {\n        min-width: 225px;\n        width: 16%;\n        align-self: flex-start;\n    }\n\n    .footer-performers {\n        @include flex-row-wrap;\n        flex: 1 1 auto;\n        align-self: center;\n    }\n\n    .sponsor-wrapper{\n        @include flex-column-wrap;\n    }\n\n    .sponsor-content{\n        color: $theme-gold;\n        margin-top: 10px;\n    }\n\n    .sponsor-link{\n        @include no-text-decoration;\n        color: $theme-dark-medium-color;\n        font-weight: bold;\n    }\n\n    .sponsor-link:hover {\n        text-decoration: underline;\n    }\n\n    .sponsor-title {\n        font-size: 11px;\n    }\n\n    .performer-wrapper{\n        @include flex-row-wrap;\n        justify-content: space-evenly;\n        align-content: flex-start;\n        flex: 1 1 auto;\n    }\n\n    .performer-content{\n        width: 185px;\n        min-height: 70px;\n    }\n\n    .hide-filter-menu{\n        display: none;\n    }\n</style>\n"]}]}