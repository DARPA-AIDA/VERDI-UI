{"remainingRequest":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/womitowoju/workspace/aida/VERDI-UI/GUI/src/components/FilterMenu.vue?vue&type=style&index=0&id=f5aac0b2&lang=scss&scoped=true&","dependencies":[{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/src/components/FilterMenu.vue","mtime":1640268464665},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/css-loader/dist/cjs.js","mtime":1641411773021},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":1641411774047},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/postcss-loader/src/index.js","mtime":1641411773350},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/sass-loader/dist/cjs.js","mtime":1641411772200},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/cache-loader/dist/cjs.js","mtime":1641411772188},{"path":"/Users/womitowoju/workspace/aida/VERDI-UI/GUI/node_modules/vue-loader/lib/index.js","mtime":1641411773606}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["FilterMenu.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuIA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"FilterMenu.vue","sourceRoot":"src/components","sourcesContent":["<!--\n  - Copyright 2019 Next Century Corporation/CACI\n  -\n  - Licensed under the Apache License, Version 2.0 (the \"License\");\n  - you may not use this file except in compliance with the License.\n  - You may obtain a copy of the License at\n  -\n  -       http://www.apache.org/licenses/LICENSE-2.0\n  -\n  - Unless required by applicable law or agreed to in writing, software\n  - distributed under the License is distributed on an \"AS IS\" BASIS,\n  - WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.\n  - See the License for the specific language governing permissions and\n  - limitations under the License.\n  -\n  -->\n\n<template>\n    <form id=\"filter_menu\" ref=\"filter_menu\" class=\"filter-menu\" @submit.prevent=\"submitFilters()\" method=\"get\">\n        <div class=\"filter-menu-content\">\n            <div class=\"filter-menu-header\">\n                <div class=\"primary-heading\">Filters</div>\n                <div>\n                    <button id=\"save_button\" disabled class=\"dark-icon-button\" @click=\"saveFilterSet()\">\n                        <v-icon>mdi-content-save-outline</v-icon>\n                    </button>\n                    <button id=\"delete_all_button\" class=\"dark-icon-button\" @click.prevent=\"resetFilterForm()\">\n                        <!--<v-icon>mdi-trash-can-outline</v-icon>-->\n                        <v-icon>mdi-refresh</v-icon>\n                    </button>\n                </div>\n            </div>\n            <div ref=\"filter_save_note\" class=\"filter-save-note\">\n                Your filters have been saved.\n            </div>\n            <div class=\"filter-menu-filters\">\n                <ExploreFilterController v-if=\"filterRoute === VUEROUTES.ta2\" ref=\"explore_filter_controller\" :showDateTooltip=\"showDateTooltip\"></ExploreFilterController>\n                <ClaimFilterController v-if=\"filterRoute === VUEROUTES.claims\" ref=\"claim_filter_controller\"></ClaimFilterController>\n            </div>\n            <div class=\"filter-submit-button-wrapper\">\n                <button id=\"submit_button\" type=\"submit\" class=\"dark-apply-button\" @click=\"deactivateFormValidation()\">\n                    Apply Filter\n                </button>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\n\n    import ExploreFilterController from '@/components/ExploreFilterController';\n    import ClaimFilterController from '@/components/ClaimFilterController';\n    import RouteParametersMixin from '@/mixins/RouteParametersMixin';\n    import QuerySubmissionMixin from '@/mixins/QuerySubmissionMixin';\n    import {PARAMETERNAMES, VUEROUTES} from '../utils';\n    import {mapMutations, mapGetters, mapState} from 'vuex';\n\n    let filterSaveNoteElement = null;\n\n    export default {\n        name: 'FilterMenu',\n        mixins: [QuerySubmissionMixin, RouteParametersMixin],\n        data: () => ({\n            showSaveOptions: false,\n            showDateTooltip: false\n        }),\n        components: {ExploreFilterController, ClaimFilterController},\n        created() {\n            this.VUEROUTES = VUEROUTES;\n        },\n        mounted() {\n            filterSaveNoteElement = this.$refs.filter_save_note;\n        },\n        computed: {\n            ...mapState(['filterReset']),\n            ...mapGetters(['getFilterKeys']),\n            filterRoute() {\n                return this.$route.name;\n            }\n        },\n        methods: {\n            ...mapMutations(['updateSavedFilters', 'updateFilterReset', 'updateFilterSubmission']),\n            saveFilterSet() {\n                this.showSaveOptions = false;\n                this.flashSaveNote();\n            },\n            resetFilterForm() {\n                if(this.filterRoute === VUEROUTES.ta2) {\n                    this.$store.commit('removeAllFilters');\n                    this.updateRouteParams(PARAMETERNAMES.filter);\n                    this.showDateTooltip = false;\n                    this.updateQueryResults();\n                }\n\n                this.updateFilterReset(true);\n            },\n            flashSaveNote() {\n                if(filterSaveNoteElement && !filterSaveNoteElement.classList.contains('opacity-filled-show')) {\n                    filterSaveNoteElement.classList.add('opacity-filled-show');\n                    setTimeout(()=>(filterSaveNoteElement.classList.remove('opacity-filled-show')), 5000);\n                }\n            },\n            deactivateFormValidation() {\n                if(this.filterRoute === VUEROUTES.ta2) {\n                    this.$refs.explore_filter_controller.deactivateComponentValidation();\n                }\n\n            },\n            submitFilters() {\n                //if there is no date filter that means the date filter is invalid and the tooltip needs to be displayed to alert the user\n                //TODO: Add functionality back in once date range filtering has been implemented\n                /*                if(!this.$store.getters.getFilterByType(DATATYPES.dateRange.id) && !this.filterReset) {\n                    this.showDateTooltip = true;\n                }\n                else {\n                    let timestamp = new Date();\n                    this.updateRouteParams(PARAMETERNAMES.filter, timestamp.getTime());\n                }*/\n\n                //Only add/update the filterId and savedFilters if valid filters exist\n                if (this.getFilterKeys.length > 0) {\n                    let timestamp = new Date();\n                    this.updateRouteParams(PARAMETERNAMES.filter, timestamp.getTime());\n                    this.updateSavedFilters({id: timestamp.getTime()});\n                } else {\n                    this.updateRouteParams(PARAMETERNAMES.filter);\n                }\n                this.queryData(this.selectedTab.toLowerCase());\n                this.updateFilterSubmission(true);\n            }\n        }\n    }\n</script>\n\n<style lang=\"scss\" scoped>\n    .filter-menu {\n        position: relative;\n        background-color: white;\n        z-index: 2;\n        box-shadow: $heavyweight-box-shadow;\n        height: 100%;\n        width: 380px;\n        min-height: $full-page-min-height;\n    }\n\n    .filter-menu-content {\n        background-color: #ffffff;\n        max-height: 100%;\n        min-height: 238px;\n        display: flex;\n        flex-direction: column;\n        position: relative;\n    }\n\n    .filter-menu-header {\n        padding: 20px 8px 0;\n        @include flex-row-nowrap;\n        color: $theme-primary-color;\n        justify-content: space-between;\n        z-index: 4;\n        border-bottom: 1px solid $theme-break-line-color;\n        height: $filter-menu-header-height;\n        align-items: center;\n\n        h4{\n            flex: 1 1 auto;\n            align-self: center;\n        }\n    }\n\n    .filter-menu-filters {\n        @include flex-column-nowrap;\n        min-height: 115px;\n        padding: 8px 0;\n        max-height: 100%;\n        clear: both;\n    }\n\n    .filter-save-menu{\n        position: absolute;\n        top: 45px;\n        z-index: 5;\n    }\n\n    .filter-save-note{\n        position: absolute;\n        z-index: 5;\n        top: 46px;\n        left: 0;\n        right: 0;\n        background: #fde073;\n        text-align: center;\n        line-height: 2.5;\n        overflow: hidden;\n        box-shadow:  0 6px 30px 5px rgba(0, 0, 0, 0.14);\n        opacity:0;\n        transition: opacity 0.6s, visibility 0.6s;\n        visibility:hidden;\n    }\n\n    .filter-submit-button-wrapper{\n        width: 100%;\n        display: flex;\n        align-content: center;\n        justify-content: center;\n        position: relative;\n        margin: 28px 0 14px;\n\n        .dark-apply-button {\n            width: 90%;\n            margin: auto;\n            @extend .dark-teal-submit-button\n        }\n    }\n\n    .filter-list-header-wrapper{\n        @include top-bottom-margin;\n        display: flex;\n        justify-content: space-between;\n        border-bottom: 0 none;\n        box-shadow: $middleweight-box-shadow;\n\n        .filter-list-header{\n            @include flex-column-nowrap;\n            padding: 6px;\n        }\n    }\n\n\n</style>\n"]}]}