prefix aida: <https://raw.githubusercontent.com/NextCenturyCorporation/AIDA-Interchange-Format/master/java/src/main/resources/com/ncc/aif/ontologies/InterchangeOntology#>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

select (GROUP_CONCAT(DISTINCT ?doc;SEPARATOR=",") as ?docs) {$from}

where {
    ?rdf_statement rdf:predicate rdf:type .
    ?rdf_statement rdf:subject ?node .
    ?rdf_statement aida:justifiedBy / aida:containedJustification? / aida:sourceDocument ?doc .
}
